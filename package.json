{
    "name": "milatiparis-optimizer",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "npx dotenvx run -- next dev",
        "build": "npx dotenvx run -f .env.production -- next build",
        "start": "npx dotenvx run -f .env.production -- next start",
        "lint": "npx dotenvx run -- next lint",
        "encrypt": "npx dotenvx encrypt -f .env && npx dotenvx encrypt -f .env.production",
        "decrypt": "npx dotenvx decrypt -f .env && npx dotenvx decrypt -f .env.production"
    },
    "dependencies": {
        "@dotenvx/dotenvx": "^1.48.4",
        "@heroicons/react": "^2.2.0",
        "@playwright/test": "^1.54.2",
        "@tailwindcss/vite": "^4.0.0",
        "@tiptap/extension-color": "^3.1.0",
        "@tiptap/extension-list-item": "^3.1.0",
        "@tiptap/extension-table": "^3.1.0",
        "@tiptap/extension-table-cell": "^3.1.0",
        "@tiptap/extension-table-header": "^3.1.0",
        "@tiptap/extension-table-row": "^3.1.0",
        "@tiptap/extension-text-style": "^3.1.0",
        "@tiptap/react": "^3.1.0",
        "@tiptap/starter-kit": "^3.1.0",
        "autoprefixer": "^10.4.21",
        "next": "15.4.6",
        "next-auth": "^4.24.8",
        "openai": "^5.12.2",
        "pg": "^8.11.3",
        "react": "^18",
        "react-dom": "^18"
    },
    "devDependencies": {
        "@tailwindcss/postcss": "^4.1.11",
        "@types/node": "^20",
        "@types/pg": "^8.10.9",
        "@types/react": "^18",
        "@types/react-dom": "^18",
        "eslint": "^8",
        "eslint-config-next": "15.4.6",
        "postcss": "^8",
        "tailwindcss": "^4.0.0",
        "typescript": "^5"
    }
}
